{
  "rules": {
    "indent": [ 2, 2, { "SwitchCase": 1 } ],
    "linebreak-style": [ 2, "unix" ],
    "no-console": 0,
    "no-unused-vars": [ 2, { "args": "after-used", "argsIgnorePattern": "^_" } ],
    "no-var": 2,
    "prefer-const": 2,
    "quotes": [ 2, "double" ],
    "semi": [ 2, "always" ],
  },
  "env": {
    "es6": true,
    "mocha": true,
    "node": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "extends": "eslint:recommended"
}
